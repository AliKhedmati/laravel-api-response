{
    "name": "alikhedmati/api-response",
    "description": "Laravel API response.",
    "autoload": {
        "psr-4": {
            "Alikhedmati\\ApiResponse\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ali Khedmati",
            "email": "ali@khedmati.ir"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "illuminate/http": "^5.5",
        "illuminate/support": "^5.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AliKhedmati\\APIResponse\\APIResponseServiceProvider"
            ],
            "aliases": {
                "API": "AliKhedmati\\APIResponse\\Facades\\APIResponse"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
